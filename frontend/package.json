{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "npm-run-all -p watch-compile-sass start-js",
    "start-js": "react-scripts start",
    "build": "npm run build-sass && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch-compile-sass": "npm run build-sass && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-sass": "node-sass-chokidar src/ -o src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
